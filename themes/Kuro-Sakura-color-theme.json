{
	"name": "Kuro Sakura",
	"type": "dark",
	"semanticHighlighting": true,
	"colors": {
		"editor.background": "#1a1b26",
		"editor.foreground": "#c0caf5",
		"editorCursor.foreground": "#e06c75",
		"editorLineNumber.foreground": "#565f89",
		"editorLineNumber.activeForeground": "#e06c75",
		"editor.selectionBackground": "#2c3043",
		"activityBar.background": "#1a1b26",
		"sideBar.background": "#16161e",
		"statusBar.background": "#16161e",
		"statusBar.foreground": "#c0caf5",
		"titleBar.activeBackground": "#1f2335",
		"tab.activeBackground": "#1f2335",
		"tab.inactiveBackground": "#1a1b26",
		"terminal.background": "#1a1b26",
		"terminal.foreground": "#c0caf5",
		"terminal.ansiBlack": "#1a1b26",
		"terminal.ansiRed": "#e06c75",
		"terminal.ansiGreen": "#98c379",
		"terminal.ansiYellow": "#e5c07b",
		"terminal.ansiBlue": "#61afef",
		"terminal.ansiMagenta": "#9d4edd",
		"terminal.ansiCyan": "#56b6c2",
		"terminal.ansiWhite": "#c0caf5",
		"editorWhitespace.foreground": "#2c3043",
		"editorIndentGuide.background1": "#2c3043",
		"editorIndentGuide.activeBackground1": "#3b3f5c",
		"editorBracketMatch.background": "#2c3043",
		"editorBracketMatch.border": "#9d4edd"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#5c6370",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
				"keyword.control"
			],
			"settings": {
				"foreground": "#9d4edd",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"variable.function",
				"support.function",
				"meta.function-call",
				"entity.name.method.js",
				"meta.class-method.js entity.name.function.js"
			],
			"settings": {
				"foreground": "#61afef"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"constant.other.symbol",
				"string.other.link"
			],
			"settings": {
				"foreground": "#98c379"
			}
		},
		{
			"name": "Numbers / Constants",
			"scope": [
				"constant.numeric",
				"support.constant",
				"constant.character",
				"constant.escape"
			],
			"settings": {
				"foreground": "#e5c07b"
			}
		},
		{
			"name": "Operators / Punctuation",
			"scope": [
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"keyword.operator"
			],
			"settings": {
				"foreground": "#61afef"
			}
		},
		{
			"name": "HTML / XML Tags",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml"
			],
			"settings": {
				"foreground": "#e06c75"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name",
				"text.html.basic entity.other.attribute-name.html",
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name"
			],
			"settings": {
				"foreground": "#9d4edd"
			}
		},
		{
			"name": "Inserted / Added",
			"scope": [
				"markup.inserted"
			],
			"settings": {
				"foreground": "#98c379"
			}
		},
		{
			"name": "Deleted / Removed",
			"scope": [
				"markup.deleted"
			],
			"settings": {
				"foreground": "#e06c75"
			}
		},
		{
			"name": "Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#9d4edd"
			}
		},
		{
			"name": "Regex / Escape",
			"scope": [
				"string.regexp",
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#56b6c2"
			}
		},
		{
			"name": "Links / URLs (fixed scopes)",
			"scope": [
				"markup.underline.link",
				"string.other.link",
				"markup.anchor",
				"constant.other.reference.link",
				"meta.link.inline"
			],
			"settings": {
				"foreground": "#61afef",
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markdown - Headings",
			"scope": [
				"markdown.heading",
				"markup.heading.markdown",
				"punctuation.definition.heading.markdown"
			],
			"settings": {
				"foreground": "#e5c07b"
			}
		},
		{
			"name": "Markdown - Italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"foreground": "#e06c75",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown - Bold",
			"scope": [
				"markup.bold",
				"markup.bold string"
			],
			"settings": {
				"foreground": "#9d4edd",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown - Links",
			"scope": [
				"string.other.link.title.markdown",
				"string.other.link.description.title.markdown",
				"constant.other.reference.link.markdown"
			],
			"settings": {
				"foreground": "#61afef"
			}
		},
		{
			"name": "JSON Keys",
			"scope": [
				"support.type.property-name.json",
				"meta.structure.dictionary.json",
				"meta.structure.dictionary.json meta.structure.dictionary.value.json"
			],
			"settings": {
				"foreground": "#9d4edd"
			}
		},
		{
			"name": "JSON Values",
			"scope": [
				"string.quoted.json",
				"constant.numeric.json",
				"constant.language.json"
			],
			"settings": {
				"foreground": "#98c379"
			}
		},
		{
			"name": "CSS / HTML Class Names",
			"scope": [
				"entity.other.attribute-name.class",
				"source.css entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#61afef"
			}
		},
		{
			"name": "CSS / HTML IDs",
			"scope": [
				"source.css entity.other.attribute-name.id",
				"source.sass entity.other.attribute-name.id"
			],
			"settings": {
				"foreground": "#e06c75"
			}
		},
		{
			"name": "Built-in / Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"support.type",
				"support.class",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#e5c07b"
			}
		},
		{
			"name": "Special / Deprecated",
			"scope": [
				"invalid",
				"invalid.illegal",
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#ff5555"
			}
		}
	]
}