{
	"name": "Shiro Sakura (Light)",
	"type": "light",
	"semanticHighlighting": true,
	"colors": {
		"editor.background": "#f9f9fb",
		"editor.foreground": "#2d2a2e",
		"editorCursor.foreground": "#d73a49",
		"editorLineNumber.foreground": "#b0b0b5",
		"editorLineNumber.activeForeground": "#9d4edd",
		"editor.selectionBackground": "#e8e3f9",
		"activityBar.background": "#f2f2f7",
		"sideBar.background": "#f6f6fa",
		"statusBar.background": "#ffffff",
		"statusBar.foreground": "#2d2a2e",
		"titleBar.activeBackground": "#f2f2f7",
		"tab.activeBackground": "#ffffff",
		"tab.inactiveBackground": "#f6f6fa",
		"terminal.background": "#ffffff",
		"terminal.foreground": "#2d2a2e",
		"terminal.ansiBlack": "#1a1b26",
		"terminal.ansiRed": "#d73a49",
		"terminal.ansiGreen": "#22863a",
		"terminal.ansiYellow": "#b08800",
		"terminal.ansiBlue": "#0366d6",
		"terminal.ansiMagenta": "#9d4edd",
		"terminal.ansiCyan": "#1b9aaa",
		"terminal.ansiWhite": "#2d2a2e",
		"editorWhitespace.foreground": "#e0e0e0",
		"editorIndentGuide.background1": "#e0e0e0",
		"editorIndentGuide.activeBackground1": "#c8c8d8",
		"editorBracketMatch.background": "#f0e9ff",
		"editorBracketMatch.border": "#9d4edd"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#7d7d7d",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
				"keyword.control"
			],
			"settings": {
				"foreground": "#9d4edd",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"variable.function",
				"support.function",
				"meta.function-call",
				"entity.name.method.js",
				"meta.class-method.js entity.name.function.js"
			],
			"settings": {
				"foreground": "#0366d6"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"constant.other.symbol",
				"string.other.link"
			],
			"settings": {
				"foreground": "#22863a"
			}
		},
		{
			"name": "Numbers / Constants",
			"scope": [
				"constant.numeric",
				"support.constant",
				"constant.character",
				"constant.escape"
			],
			"settings": {
				"foreground": "#b08800"
			}
		},
		{
			"name": "Operators / Punctuation",
			"scope": [
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"keyword.operator"
			],
			"settings": {
				"foreground": "#0366d6"
			}
		},
		{
			"name": "HTML / XML Tags",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml"
			],
			"settings": {
				"foreground": "#d73a49"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name",
				"text.html.basic entity.other.attribute-name.html",
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name"
			],
			"settings": {
				"foreground": "#9d4edd"
			}
		},
		{
			"name": "Inserted / Added",
			"scope": [
				"markup.inserted"
			],
			"settings": {
				"foreground": "#22863a"
			}
		},
		{
			"name": "Deleted / Removed",
			"scope": [
				"markup.deleted"
			],
			"settings": {
				"foreground": "#d73a49"
			}
		},
		{
			"name": "Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#9d4edd"
			}
		},
		{
			"name": "Regex / Escape",
			"scope": [
				"string.regexp",
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#1b9aaa"
			}
		},
		{
			"name": "Links / URLs (fixed scopes)",
			"scope": [
				"markup.underline.link",
				"string.other.link",
				"markup.anchor",
				"constant.other.reference.link",
				"meta.link.inline"
			],
			"settings": {
				"foreground": "#0366d6",
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markdown - Headings",
			"scope": [
				"markdown.heading",
				"markup.heading.markdown",
				"punctuation.definition.heading.markdown"
			],
			"settings": {
				"foreground": "#b08800"
			}
		},
		{
			"name": "Markdown - Italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"foreground": "#d73a49",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown - Bold",
			"scope": [
				"markup.bold",
				"markup.bold string"
			],
			"settings": {
				"foreground": "#9d4edd",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown - Links",
			"scope": [
				"string.other.link.title.markdown",
				"string.other.link.description.title.markdown",
				"constant.other.reference.link.markdown"
			],
			"settings": {
				"foreground": "#0366d6"
			}
		},
		{
			"name": "JSON Keys",
			"scope": [
				"support.type.property-name.json",
				"meta.structure.dictionary.json",
				"meta.structure.dictionary.json meta.structure.dictionary.value.json"
			],
			"settings": {
				"foreground": "#9d4edd"
			}
		},
		{
			"name": "JSON Values",
			"scope": [
				"string.quoted.json",
				"constant.numeric.json",
				"constant.language.json"
			],
			"settings": {
				"foreground": "#22863a"
			}
		},
		{
			"name": "CSS / HTML Class Names",
			"scope": [
				"entity.other.attribute-name.class",
				"source.css entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#0366d6"
			}
		},
		{
			"name": "CSS / HTML IDs",
			"scope": [
				"source.css entity.other.attribute-name.id",
				"source.sass entity.other.attribute-name.id"
			],
			"settings": {
				"foreground": "#d73a49"
			}
		},
		{
			"name": "Built-in / Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"support.type",
				"support.class",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#b08800"
			}
		},
		{
			"name": "Special / Deprecated",
			"scope": [
				"invalid",
				"invalid.illegal",
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#ff5555"
			}
		}
	]
}